{
  "request": {
    "method": "POST",
    "urlPath": "/orders",
    "headers": {
      "authorization": {
        "equalTo": "PrintApiKey test_key_12345"
      }
    }
  },
  "response": {
    "transformers": ["response-template"],
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "Access-Control-Allow-Origin": "*"
    },
    "bodyFileName": "order.60001234.json"
  },
  "serveEventListeners": [
    {
      "name": "webhook",
      "parameters": {
        "method": "POST",
        "url": "{{jsonPath originalRequest.body '$.webhookUrl'}}",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{ \"result\": \"SUCCESS\" }"
      }
    }
  ]
}
